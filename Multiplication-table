

import java.util.*;
import java.lang.*;
import java.io.*;


class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    
	 for(int num=1 ;num <= 12; ++num){
        for(int i = 1; i <= 12; ++i)
        {
            System.out.printf("%d * %d = %d \n", num, i, num * i);
        }
        System.out.println("-------------------------------------------");
	 }
    }
}
